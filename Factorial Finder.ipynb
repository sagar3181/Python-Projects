    "factorial = 1\n",
    "if user_input == 0:\n",
    "    print(\"please enter a digit bigger than zero\")\n",
    "else: \n",
    "    for i in range(1,user_input+1):\n",
    "        factorial = factorial * i\n",
    "        print(factorial)\n"
